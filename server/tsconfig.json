{
  "compilerOptions": {
    "target": "ES2023",
    "module": "ESNext",
    "moduleResolution": "Node",

    "rootDir": ".",
    "outDir": "./dist",

    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src", "lib", "src/app/middlewares"],
  "exclude": ["node_modules", "dist", "prisma"]
}
